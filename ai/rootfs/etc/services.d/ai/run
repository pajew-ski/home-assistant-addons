#!/usr/bin/with-contenv bashio
# ==============================================================================
# Start the example service
# s6-overlay docs: https://github.com/just-containers/s6-overlay
# ==============================================================================

# Add your code here

# Declare variables
declare OPENAI_API_KEY

## Get the 'OPENAI_API_KEY' key from the user config options.
OPENAI_API_KEY=$(bashio::config 'OPENAI_API_KEY')

## Print the OPENAI_API_KEY the user supplied, defaults to "Hello World..."
bashio::log.info "${OPENAI_API_KEY:="sk-"}"

## Run your program
exec /usr/bin/my_program
